<mjml background-color="#f2f2f2" padding="0">
  <mj-head>
    <mj-title>2023</mj-title>
    <mj-preview></mj-preview>
    <mj-style inline="inline">
      * {
        line-height: 1.6;
      }

      h1, h2, h3:not(.quote), h4 {
        font-weight: bold;
        margin-bottom: 5px;
        letter-spacing: .7px;
        text-transform: uppercase;
      }

      ul, p {
        margin-top: 0;
      }

      .sub-title-w {
        color: #3b2f91;
      }

      .ending {
        font-size: 17px;
      }

      .title-w {
        padding: 4px;
        border: 1px solid white;
        display: inline-block;
        padding-left: 20px;
        padding-right: 20px;
        text-transform: uppercase;
        letter-spacing: 1px;
        margin-top: 2px;
        margin-bottom: 2px;
        font-weight: 500;
      }

      h3.quote {
        text-transform: none;
      }

      .sub-title-d {
        color: #008bd0;
      }

      .title-d {
        padding: 4px;
        border: 1px solid white;
        display: inline-block;
        padding-left: 20px;
        padding-right: 20px;
        text-transform: uppercase;
        letter-spacing: 1px;
        margin-top: 2px;
        margin-bottom: 2px;
        font-weight: 500;
      }

      .bijschrift {
        font-size: .9em;
        color: gray;
      }

      ul {
        padding-left: 20px;
      }

      hr {
        background: none;
        border-bottom: none;
        display: none;
        border-top: 1px dashed #dddddd;
      }

      html {
        background-color: #f2f2f2;
      }

      .link-nostyle {
        color: inherit;
        text-decoration: none
      }
    </mj-style>
  </mj-head>
  <mj-body font-family="Arial" padding="0" background-color="#f2f2f2">
